XF86Audio{Prev,Next}
	playerctl {previous,next}

XF86AudioPlay
	playerctl --player=spotify play-pause

XF86Audio{Lower,Raise}Volume
    amixer -c 1 sset PCM 2%{-,+}

XF86AudioMute
    amixer -c 1 sset PCM toggle

XF86MonBrightness{Down,Up}
    xbacklight -{dec,inc} 5

Print
    gnome-screenshot -f ~/Pictures/screenshot/"$(date '+%Y-%m-%d_%H-%M-%S').png"

mod4 + shift + k
    $HOME/.config/dmenu_script/pkill_process.sh

mod4 + shift + c
    gnome-calculator
