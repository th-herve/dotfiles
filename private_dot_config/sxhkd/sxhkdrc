XF86Audio{Prev,Next}
	playerctl {previous,next}

XF86AudioPlay
	playerctl play-pause

XF86Audio{Lower,Raise}Volume
    amixer -c 0 sset Master 2%{-,+}

XF86AudioMute
    pactl set-sink-mute 0 toggle

XF86MonBrightness{Down,Up}
    xbacklight -{dec,inc} 1

Print
    gnome-screenshot -f ~/Pictures/screenshot/"$(date '+%Y-%m-%d_%H-%M-%S').png"

mod4 + shift + k
    $HOME/.config/dmenu_script/pkill_process.sh

mod4 + shift + c
    GTK_THEME=Orchis-Dark-Nord gnome-calculator

